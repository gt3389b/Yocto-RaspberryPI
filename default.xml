<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote fetch="git://git.openembedded.org" name="oe" review=""/>
    <remote fetch="git://git.yoctoproject.org" name="yocto" review=""/>
    <remote fetch="git://github.com/meta-qt5" name="qt5" review=""/>
    <remote fetch="https://github.com/amirna2" name="amirna2" review=""/>
    
    <!-- List all the repositories you want to fetch -->
    <!-- "name"     git repository's name on hosting server
         "remote"   the remote host from above (if not the default)
         "revision" commitish to checkout (if not the default)
         "path"     directory where git repository gets cloned/synced 
    -->   
    <project path="poky" 		remote="yocto" name="poky" branch="krogoth" revision="master" />  
    <project path="meta-openembedded"   remote="oe" name="meta-openembedded" revision="master" />
    <project path="meta-qt5"      	remote="qt5" name="meta-qt5" revision="master" />
    <project path="meta-raspberrypi"    remote="yocto" name="meta-raspberrypi" revision="master" />
    <project path="buildenv"    	remote="amirna2" name="buildenv" revision="master" />
      
</manifest>
